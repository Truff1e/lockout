{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/golden_carrot"
    },
    "cases": [
      {
        "when": "T0003",
        "model": {
          "type": "model",
          "model": "item/tame_horse"
        }
      },
      {
        "when": "B0008",
        "model": {
          "type": "model",
          "model": "item/breed_horse"
        }
      }
    ]
  }
}
